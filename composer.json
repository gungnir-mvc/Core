{
    "name": "gungnir-mvc/core",
    "description": "Core library for gungnir-mvc",
    "type": "library",
    "require": {
      "gungnir-mvc/event": "dev-master"
    },
        "require-dev": {
                "phpunit/phpunit": "5.5.*",
                "phpspec/prophecy": "~1.0",
                "mikey179/vfsStream": "~1.6"
                },
        "repositories": [
          {
            "type": "vcs",
            "url": "https://github.com/gungnir-mvc/event.git"
          }
        ],
        "autoload": {
        "psr-4": { "Gungnir\\Core\\": "src/" },
        "exclude-from-classmap": [
            "/tests/"
        ]
    }
}
